<odoo>
    <record id="partner_communication_channel_view_tree" model="ir.ui.view">
        <field name="name">partner.communication.channel.view.tree</field>
        <field name="model">partner.communication.channel</field>
        <field name="arch" type="xml">
            <tree string="Communication Channel" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="action_partner_communication_channel" model="ir.actions.act_window">
        <field name="name">Communication Channel</field>
        <field name="res_model">partner.communication.channel</field>
        <field name="view_mode">tree</field>
    </record>
    <menuitem id="menu_partner_communication_channel"
        action="action_partner_communication_channel"
        name="Communication Channel" parent="contacts.res_partner_menu_config"
        sequence="99"/>
</odoo>
